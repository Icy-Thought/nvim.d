{
    "pkgs-template": {
        "prefix": "ptmp",
        "body": [
            "{ stdenv, lib, fetchFromGitHub}:",
            "stdenv.mkDerivation rec {",
              "pname = '';",
              "version = '';",
            
              "src = fetchFromGitHub {",
              "  owner = '';",
              "  repo = '';",
              "  rev = '';",
              "  sha256 = '';",
              "};",
            
              "buildInputs = [ ];",
            
              "nativeBuildInputs = [ ];",
            
              "meta = with lib; {",
              "  description = '';",
              "  homepage = '';",
              "  license = licenses.;",
              "  platforms = platforms.linux;",
              "  maintainers = with maintainers; [ icy-thought ];",
              "};",
            "}",
        ],
        "description": "A simple template required for nix-packaing."
    },
    "let": {
        "prefix": "let",
        "body": [
            "let",
            "  $1",
            "in"
        ],
        "description": "Insert a `let ... in` block"
    }
}
